{
  "name": "@gdbots/schemas",
  "version": "0.1.0",
  "description": "Sample schemas for common gdbots services.",
  "repository": {
    "type": "git",
    "url": "https://github.com/gdbots/acme-schemas.git"
  },
  "license": "Apache-2.0",
  "scripts": {
    "build": "BABEL_ENV=es6 babel ./build/js/src --out-dir ./build/js/dist",
    "clean": "rimraf ./build/js/dist",
    "lint": "BABEL_ENV=cjs eslint ./build/js/src ./build/js/tests",
    "prepublish": "npm run clean && npm run lint && npm run test && npm run build",
    "test": "BABEL_ENV=cjs tape -r ./build/js/tests/babel-register.js './build/js/tests/**/*.test.js' | faucet"
  },
  "module": "./index.js",
  "jsnext:main": "./index.js",
  "dependencies": {
    "@gdbots/common": "^0.1.0",
    "@gdbots/pbj": "git://github.com/gdbots/pbj-js.git#temp"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-preset-env": "^1.4.0",
    "babel-register": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "faucet": "^0.0.1",
    "rimraf": "^2.6.1",
    "tape": "^4.7.0"
  }
}
