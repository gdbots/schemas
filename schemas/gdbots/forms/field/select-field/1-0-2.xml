<?xml version="1.0" encoding="UTF-8" ?>
<pbj-schema xmlns="http://gdbots.io/pbj/xsd"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://gdbots.io/pbj/xsd http://gdbots.io/pbj/xsd/schema.xsd">

  <schema id="pbj:gdbots:forms:field:select-field:1-0-2">
    <fields>
      <field name="option_labels" type="string" rule="list"/>
      <field name="option_values" type="string" rule="list"/>
      <field name="allow_other" type="boolean"/>
      <field name="allow_multiple" type="boolean"/>
    </fields>

    <mixins>
      <curie-major>gdbots:forms:mixin:field:v1</curie-major>
    </mixins>
  </schema>
</pbj-schema>
