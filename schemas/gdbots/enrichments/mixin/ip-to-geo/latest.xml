<?xml version="1.0" encoding="UTF-8" ?>
<pbj-schema xmlns="http://gdbots.io/pbj/xsd"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://gdbots.io/pbj/xsd http://gdbots.io/pbj/xsd/schema.xsd">

  <schema id="pbj:gdbots:enrichments:mixin:ip-to-geo:1-0-0" mixin="true">
    <fields>
      <field name="ctx_ip" type="string" format="ipv4" overridable="true"/>
      <field name="ctx_ip_geo" type="message">
        <any-of>
          <curie>gdbots:geo::address</curie>
        </any-of>
      </field>
    </fields>

    <php-options>
      <namespace>Gdbots\Schemas\Enrichments\Mixin\IpToGeo</namespace>
    </php-options>
  </schema>
</pbj-schema>
