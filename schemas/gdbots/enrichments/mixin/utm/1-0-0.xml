<?xml version="1.0" encoding="UTF-8" ?>
<pbj-schema xmlns="http://gdbots.io/pbj/xsd"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://gdbots.io/pbj/xsd http://gdbots.io/pbj/xsd/schema.xsd">

  <schema id="pbj:gdbots:enrichments:mixin:utm:1-0-0" mixin="true">
    <fields>
      <field name="utm_source" type="string" pattern="^[\w\/\.:-]+$" max="50"/>
      <field name="utm_medium" type="string" pattern="^[\w\/\.:-]+$" max="50"/>
      <field name="utm_term" type="string" pattern="^[\w\s\/\.,:-]+$" max="100"/>
      <field name="utm_content" type="string" pattern="^[\w\/\.:-]+$" max="50"/>
      <field name="utm_campaign" type="string" pattern="^[\w\/\.:-]+$" max="50"/>
    </fields>
  </schema>
</pbj-schema>
