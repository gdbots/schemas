<?xml version="1.0" encoding="UTF-8" ?>
<pbj-schema xmlns="http://gdbots.io/pbj/xsd"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://gdbots.io/pbj/xsd http://gdbots.io/pbj/xsd/schema.xsd">

  <schema id="pbj:gdbots:ncr:event:labels-updated:1-0-0">
    <fields>
      <field name="labels_added" type="string" rule="set"/>
      <field name="labels_removed" type="string" rule="set"/>
    </fields>

    <mixins>
      <curie-major>gdbots:pbjx:mixin:event:v1</curie-major>
      <curie-major>gdbots:common:mixin:taggable:v1</curie-major>
    </mixins>
  </schema>
</pbj-schema>
