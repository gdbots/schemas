<?xml version="1.0" encoding="UTF-8" ?>
<pbj-schema xmlns="http://gdbots.io/pbj/xsd"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://gdbots.io/pbj/xsd http://gdbots.io/pbj/xsd/schema.xsd">

  <schema id="pbj:gdbots:ncr:mixin:get-node-batch-response:1-0-0" mixin="true" extends="gdbots:pbjx:mixin:response:v1">
    <fields>
      <field name="nodes" type="message" rule="map" overridable="true">
        <any-of>
          <curie>gdbots:ncr:mixin:node</curie>
        </any-of>
      </field>
      <field name="missing_node_refs" type="identifier" rule="set">
        <description>
          The "missing_node_refs" field contains a set of node_refs that
          the batch request failed to retrieve.
        </description>
        <php-options>
          <imports>use Gdbots\Schemas\Ncr\NodeRef;</imports>
          <class-name>NodeRef::class</class-name>
        </php-options>
        <js-options>
          <imports>import NodeRef from '@gdbots/schemas/gdbots/ncr/NodeRef';</imports>
          <class-proto>NodeRef</class-proto>
        </js-options>
      </field>
    </fields>
  </schema>
</pbj-schema>
