<?xml version="1.0" encoding="UTF-8" ?>
<pbj-schema xmlns="http://gdbots.io/pbj/xsd"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://gdbots.io/pbj/xsd http://gdbots.io/pbj/xsd/schema.xsd">

  <schema id="pbj:gdbots:ncr:mixin:node-created:1-0-0" mixin="true" extends="gdbots:pbjx:mixin:event:v1">
    <fields>
      <field name="node" type="message" required="true" overridable="true">
        <any-of>
          <curie>gdbots:ncr:mixin:node</curie>
        </any-of>
      </field>
    </fields>

    <php-options>
      <namespace>Gdbots\Schemas\Ncr\Mixin\NodeCreated</namespace>
    </php-options>
  </schema>
</pbj-schema>
