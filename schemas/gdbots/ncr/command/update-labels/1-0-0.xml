<?xml version="1.0" encoding="UTF-8" ?>
<pbj-schema xmlns="http://gdbots.io/pbj/xsd"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://gdbots.io/pbj/xsd http://gdbots.io/pbj/xsd/schema.xsd">

  <schema id="pbj:gdbots:ncr:command:update-labels:1-0-0">
    <fields>
      <field name="add_labels" type="string" rule="list" required="true"/>
      <field name="remove_labels" type="string" rule="list" required="true" overridable="true"/>
    </fields>

    <mixins>
      <curie-major>gdbots:pbjx:mixin:command:v1</curie-major>
      <curie-major>gdbots:common:mixin:labelable:v1</curie-major>
    </mixins>
  </schema>
</pbj-schema>
