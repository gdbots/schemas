<?xml version="1.0" encoding="UTF-8" ?>
<pbj-enums xmlns="http://gdbots.io/pbj/xsd"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://gdbots.io/pbj/xsd http://gdbots.io/pbj/xsd/enums.xsd">

  <enums namespace="gdbots:common">
    <enum name="day-of-week" type="int">
      <option key="UNKNOWN" value="0"/>
      <option key="SUNDAY" value="0"/>
      <option key="MONDAY" value="1"/>
      <option key="TUESDAY" value="2"/>
      <option key="WEDNESDAY" value="3"/>
      <option key="THURSDAY" value="4"/>
      <option key="FRIDAY" value="5"/>
      <option key="SATURDAY" value="6"/>
      <option key="SUNDAY_TOO" value="7"/>
    </enum>

    <!--
      @link https://en.wikipedia.org/wiki/ISO/IEC_5218
      @link http://www.datadictionary.nhs.uk/data_dictionary/attributes/p/person/person_gender_code_de.asp?shownav=1
      @link http://www.datadictionary.nhs.uk/data_dictionary/attributes/p/person/person_stated_gender_code_de.asp?shownav=1
    -->
    <enum name="gender" type="int">
      <option key="UNKNOWN" value="0"/>
      <option key="MALE" value="1"/>
      <option key="FEMALE" value="2"/>
      <option key="NOT_SPECIFIED" value="9"/>
    </enum>

    <!--
      @link http://www.datadictionary.nhs.uk/data_dictionary/attributes/p/person/person_stated_sexual_orientation_code_de.asp?shownav=1
    -->
    <enum name="sexual-orientation" type="string">
      <option key="UNKNOWN" value="unknown"/>
      <option key="HETEROSEXUAL" value="1"/>
      <option key="HOMOSEXUAL" value="2"/>
      <option key="BISEXUAL" value="3"/>
      <option key="OTHER" value="4"/>
      <option key="NOT_SURE" value="U"/>
      <option key="NOT_SPECIFIED" value="Z"/>
    </enum>

    <enum name="month" type="int">
      <option key="UNKNOWN" value="0"/>
      <option key="JANUARY" value="1"/>
      <option key="FEBRUARY" value="2"/>
      <option key="MARCH" value="3"/>
      <option key="APRIL" value="4"/>
      <option key="MAY" value="5"/>
      <option key="JUNE" value="6"/>
      <option key="JULY" value="7"/>
      <option key="AUGUST" value="8"/>
      <option key="SEPTEMBER" value="9"/>
      <option key="OCTOBER" value="10"/>
      <option key="NOVEMBER" value="11"/>
      <option key="DECEMBER" value="12"/>
    </enum>

    <enum name="phone-type" type="string">
      <option key="UNKNOWN" value="unknown"/>
      <option key="MOBILE" value="mobile"/>
      <option key="HOME" value="home"/>
      <option key="WORK" value="work"/>
      <option key="FAX" value="fax"/>
    </enum>

